{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/trendingFrame",
      "dest": "/api/trendingFrame.js"
    },
    {
      "src": "/api/generateInitialImage",
      "dest": "/api/generateInitialImage.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
