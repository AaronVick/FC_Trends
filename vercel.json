{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/trendingFrame",
      "dest": "/api/trendingFrame.js"
    },
    {
      "src": "/api/generateInitialImage",
      "dest": "/api/generateInitialImage.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/trendingFrame.js": {
      "runtime": "edge"
    },
    "api/generateInitialImage.js": {
      "runtime": "edge"
    }
  },
  "env": {
    "HUGGING_FACE_API_KEY": "@hugging_face_api_key"
  }
}
