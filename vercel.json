{
  "version": 2,
  "functions": {
    "api/summarizeTrends.js": {
      "runtime": "@vercel/node@2.0.0"
    }
  },
  "routes": [
    { "src": "/api/summarizeTrends", "dest": "/api/summarizeTrends.js" },
    { "src": "/api/trendingFrame", "dest": "/api/trendingFrame.js" },
    { "src": "/api/generateInitialImage", "dest": "/api/generateInitialImage.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}