{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "jsx": "react",  // This ensures TypeScript can compile JSX
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  